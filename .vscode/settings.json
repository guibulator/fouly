{
  "git.ignoreLimitWarning": true,
  "appService.showBuildDuringDeployPrompt": false,
  "editor.formatOnSave": true,
  "breadcrumbs.enabled": true,
  "files.exclude": {
    "**/.gitkeep": true,
    "**/dist": false,
    "**/yarn.lock": true,
    "**/tsconfig*": true,
    "**/tslint*": true,
    "**/jest*": true,
    "lerna.json": true,
    "**/tmp": true
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  }
}
