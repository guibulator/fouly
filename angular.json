    "fouly-webfront-e2e": {
      "root": "apps/fouly/webfront-e2e",
      "sourceRoot": "apps/fouly/webfront-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
