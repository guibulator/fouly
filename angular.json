        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/fouly/webfront-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/fouly/webfront-e2e/**"]
          }
        }
      }
    },
    "fouly-pwa-e2e": {
      "root": "apps/fouly/pwa-e2e",
