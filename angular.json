            "main": "apps/fouly/backend/src/main.ts",
            "tsConfig": "apps/fouly/backend/tsconfig.app.json",
            "assets": ["apps/fouly/backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
